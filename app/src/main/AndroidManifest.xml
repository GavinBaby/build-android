<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anuode.build_andriod">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Build"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".modules.index.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".modules.index.MainActivity"/>
        <activity android:name=".modules.login.LoginActivity"/>
        <activity android:name=".modules.login.RegisterActivity"/>
        <activity android:name=".modules.login.ForgetPasswordActivity"/>
        <activity android:name=".modules.account.AboutActivity"/>
        <activity android:name=".modules.account.AccountActivity"/>
        <activity android:name=".modules.account.NickActivity"/>
        <activity android:name=".modules.account.ResetPasswordActivity"/>
    </application>

</manifest>
